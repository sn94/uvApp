<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@drawable/uvapp_secondary">



    <android.support.v7.widget.Toolbar
        android:layout_height="70dp"
        android:layout_width="match_parent"
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/my_toolbar_result"
        android:background="@color/primaryDarkColor"
        app:titleTextColor="@color/primaryTextColor"
        android:theme="@style/ThemeOverlay.AppCompat.Dark"
        app:titleTextAppearance="@style/appbartitle"
        android:layout_margin="0dp"
        />

    <TextView
        android:id="@+id/fr_nickname"
        style="@style/title_style"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/black_alpha" />

    <TextView
        style="@style/text_style"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/black_alpha"
        android:text="@string/Fototipo_result_text1" />

    <TextView
        android:id="@+id/fr_fototipo"
        style="@style/title_style"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/black_alpha" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/black_alpha"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/fr_fps_num"
            style="@style/title_style"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/Fototipo_result_text3" />

        <TextView
            android:id="@+id/fr_fps"
            style="@style/text_style"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
    </LinearLayout>

        <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/fr_features_button"
            android:onClick="ver_caracteristicas_fototipo"
            android:text="@string/Fototipo_result_button_text1"/>


    <TextView
        style="@style/text_style"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white_alpha"
        android:text="@string/Fototipo_result_text2" />

        <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="guardar_userdata"
        android:text="@string/Fototipo_visual_test_button_text2"/>

</LinearLayout>